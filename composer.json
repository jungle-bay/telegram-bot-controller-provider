{
    "name": "jungle-bay/telegram-bot-controller-provider",
    "description": "Telegram Bot Controller Provider for Silex",
    "type": "library",
    "keywords": [
        "php",
        "telegram",
        "bot",
        "controller",
        "provider",
        "silex"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roma Baranenko",
            "email": "jungle.romabb8@gmail.com",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jungle-bay/telegram-bot-controller-provider/issues/"
    },
    "require": {
        "php": ">=5.5.9",
        "silex/silex": "^2.0",
        "jungle-bay/telegram-bot-shell": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/browser-kit": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Silex\\": "src/Silex/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\": "tests/Acme/",
            "Silex\\Tests\\": "tests/Silex/Tests/"
        }
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php --filter",
        "tests": "phpunit --bootstrap vendor/autoload.php tests"
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!src/*",
            "!composer.json",
            "!license.txt"
        ]
    }
}
